{
    "manifest_version":2,
    "name":"lesson1",
    "version":"1.0",
    "description":"chajian",
    "author":"me",
    "icons":{
        "16":"img/icon.png",
        "48":"img/icon.png",
        "128":"img/icon.png"
    },
    "browser_action":{
        "default_icon":"img/icon.png",
        "default_title":"插件标题测试",
        "default_popup":"main.html"
    },
    "background":{
        "page":"back.html"
    },
    "permissions":[
        "contextMenus",
        "tabs",
        "*://*/*",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["js/baidu.js"],
            "run_at":"document_start"
        }
    ],
    "chrome_url_overrides":{
        "newtab":"new.html"
    }
}
